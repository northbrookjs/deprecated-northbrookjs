{
  "compilerOptions": {
    "declaration": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "lib": [
      "es5",
      "es2015"
    ],
    "noImplicitAny": true,
    "sourceMap": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "outDir": "./",
    "types": [
      "mocha",
      "minimist",
      "node",
      "findup-sync",
      "ramda"
    ]
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": []
}
